<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/splashScreenFragment">
  <fragment
    android:id="@+id/splashScreenFragment"
    android:name="com.mkamilmistar.gold_market.ui.SplashScreenFragment"
    android:label="fragment_splash_screen"
    tools:layout="@layout/fragment_splash_screen">
    <action
      android:id="@+id/action_splashScreenFragment_to_onBoardingFragment"
      app:destination="@id/onBoardingFragment"
      app:popUpToInclusive="true"
      app:popUpTo="@id/nav_graph"/>
  </fragment>
  <fragment
    android:id="@+id/onBoardingFragment"
    android:name="com.mkamilmistar.gold_market.ui.OnBoardingFragment"
    android:label="fragment_on_boarding"
    tools:layout="@layout/fragment_on_boarding" >
    <action
      android:id="@+id/action_onBoardingFragment_to_loginFragment"
      app:destination="@id/loginFragment"
      app:popUpToInclusive="true"
      app:popUpTo="@id/nav_graph"/>
  </fragment>
  <fragment
    android:id="@+id/loginFragment"
    android:name="com.mkamilmistar.gold_market.ui.login.LoginFragment"
    android:label="fragment_login"
    tools:layout="@layout/fragment_login" >
    <action
      android:id="@+id/action_loginFragment_to_forgetPasswordFragment"
      app:destination="@id/forgetPasswordFragment" />
    <action
      android:id="@+id/action_loginFragment_to_registerFragment"
      app:destination="@id/registerFragment" />
  </fragment>
  <fragment
    android:id="@+id/forgetPasswordFragment"
    android:name="com.mkamilmistar.gold_market.ui.ForgetPasswordFragment"
    android:label="fragment_forget_password"
    tools:layout="@layout/fragment_forget_password" />
  <fragment
    android:id="@+id/registerFragment"
    android:name="com.mkamilmistar.gold_market.ui.register.RegisterFragment"
    android:label="fragment_register"
    tools:layout="@layout/fragment_register" >
    <action
      android:id="@+id/action_registerFragment_to_termAndConditionFragment"
      app:destination="@id/termAndConditionFragment" />
    <action
      android:id="@+id/action_registerFragment_to_loginFragment"
      app:destination="@id/loginFragment" />
  </fragment>
  <fragment
    android:id="@+id/termAndConditionFragment"
    android:name="com.mkamilmistar.gold_market.ui.TermAndConditionFragment"
    android:label="fragment_term_and_condition"
    tools:layout="@layout/fragment_term_and_condition" />
  <fragment
    android:id="@+id/homeFragment"
    android:name="com.mkamilmistar.gold_market.ui.home.HomeFragment"
    android:label="fragment_home"
    tools:layout="@layout/fragment_home" >
    <action
      android:id="@+id/action_homeFragment_to_pocketFragment"
      app:destination="@id/pocketFragment" />
  </fragment>
  <fragment
    android:id="@+id/historyFragment"
    android:name="com.mkamilmistar.gold_market.ui.history.HistoryFragment"
    android:label="fragment_history"
    tools:layout="@layout/fragment_history" />
  <fragment
    android:id="@+id/settingFragment"
    android:name="com.mkamilmistar.gold_market.ui.settings.SettingFragment"
    android:label="fragment_setting"
    tools:layout="@layout/fragment_setting" >
    <action
      android:id="@+id/action_settingFragment_to_loginFragment"
      app:destination="@id/loginFragment"
      app:popUpToInclusive="true"
      app:popUpTo="@id/settingFragment"
      />
  </fragment>
  <fragment
    android:id="@+id/pocketFragment"
    android:name="com.mkamilmistar.gold_market.ui.pocket.PocketFragment"
    android:label="fragment_pocket"
    tools:layout="@layout/fragment_pocket" />
</navigation>
